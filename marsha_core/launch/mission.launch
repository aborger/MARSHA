<launch>
    <include file="$(find marsha_core)/launch/satlab_net.machine"/>

    <rosparam file="$(find marsha_core)/config/PCS.yaml"/>

    
    <include file="$(find arm2d2_hardware)/launch/arm2d2.launch">
        <arg name="ns" value="left"/>
        <arg name="jets" value="jet1"/>
        <arg name="launch_rviz" value="false"/>
    </include>
    
    <group ns="left">
        <node pkg="marsha_core" name="pcs" type="left_pcs" output="screen" machine="jet1"/>
        <node pkg="marsha_core" name="te_detect" type="te_detect" output="screen" machine="jet1"/>
    
    </group>

    

    <group ns="right">
        <node pkg="marsha_core" name="pcs" type="right_pcs" output="screen" machine="marsha"/>
        <node pkg="marsha_core" name="te_detect" type="te_detect" output="screen" machine="marsha"/>
    </group>

    <!--
    <node pkg="marsha_detection" name="record_longeron" type="record_longeron" machine="jet1"/>
    -->
</launch>