<?xml version="1.0" encoding="utf-8" ?>

<robot name="arm2d2" xmlns:xacro="http://www.ros.org/wifi/xacro">

  <xacro:arg name="include_arm_base" default="true"/>
  <xacro:arg name="ns" default="left"/>

  <!-- Convert arg to property -->
  <xacro:property name="ns" value="$(arg ns)"/>

  <material name="silver">
    <color rgba="0.700 0.700 0.700 1.000" />
  </material>

  <xacro:if value="$(arg include_arm_base)">
    <xacro:include filename="$(find arm2d2_description)/urdf/base.urdf.xacro"/>
  </xacro:if>

  <xacro:unless value="$(arg include_arm_base)">
    <link name="world"/>

    <link name="base_link">
      <inertial>
        <origin rpy="0 0 0" xyz="0.06861790086338357 -0.0835697992386399 0.07107579694115096"/>
        <mass value="4.519107238213137"/>
        <inertia ixx="0.059779" ixy="-0.0" ixz="0.0" iyy="0.062251" iyz="0.000956" izz="0.076317"/>
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <box size="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <box size="0.001 0.001 0.001"/>
        </geometry>
      </collision>
    </link>

    <joint name="world_joint" type="fixed">
      <parent link="world"/>
      <child link="base_link"/>
      <origin xyz="0 0 0.01" rpy="0 0 0"/>
    </joint>
  </xacro:unless>

  <xacro:if value="${ns == 'left'}">
    <link name="link_1">
      <inertial>
        <origin rpy="0 0 0" xyz="-0.00789643305978462 0.002474313899377526 0.01425764348212194" />
        <mass value="0.17435766910618158" />
        <inertia ixx="9.7e-05" ixy="7e-06" ixz="2.8e-05" iyy="0.000111" iyz="-1.2e-05" izz="0.000119" />
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="0.003226 0.019004 -0.084836" />
        <geometry>
          <mesh filename="package://arm2d2_description/meshes/link_1.stl" scale="0.001 0.001 0.001" />
        </geometry>
        <material name="silver" />
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="0.003226 0.019004 -0.084836" />
        <geometry>
          <mesh filename="package://arm2d2_description/meshes/link_1.stl" scale="0.001 0.001 0.001" />
        </geometry>
      </collision>
    </link>

    
    <joint name="joint_1" type="revolute">
      <origin rpy="0 0 0" xyz="-0.003226 -0.019004 0.084836" />
      <parent link="base_link" />
      <child link="link_1" />
      <axis xyz="-0.0 -0.0 1.0" />
      <limit effort="100" lower="-3.1415" upper="3.1415" velocity="100"/>
    </joint>
  </xacro:if>

  <xacro:if value="${ns == 'right'}">
    <link name="link_1">
      <inertial>
        <origin rpy="0 0 0" xyz="0.007896441922670472 0.002474313899377481 0.014257643482122176"/>
        <mass value="0.17435766910618158"/>
        <inertia ixx="9.7e-05" ixy="-7e-06" ixz="-2.8e-05" iyy="0.000111" iyz="-1.2e-05" izz="0.000119"/>
      </inertial>
      <visual>
        <origin rpy="0 0 0" xyz="-0.140459 0.019004 -0.084836"/>
        <geometry>
          <mesh filename="package://arm2d2_description/meshes/right_link_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="silver"/>
      </visual>
      <collision>
        <origin rpy="0 0 0" xyz="-0.140459 0.019004 -0.084836"/>
        <geometry>
          <mesh filename="package://arm2d2_description/meshes/right_link_1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
    </link>

    <joint name="joint_1" type="revolute">
      <origin rpy="0 0 0" xyz="0.140459 -0.019004 0.084836"/>
      <parent link="base_link"/>
      <child link="link_1"/>
      <axis xyz="-0.0 -0.0 1.0"/>
      <limit effort="100" lower="-3.1415" upper="3.1415" velocity="100"/>
    </joint>
  </xacro:if>

  <link name="link_2">
    <inertial>
      <origin rpy="0 0 0" xyz="0.01898708853636933 -8.655304474807987e-06 0.04227958883013139" />
      <mass value="0.3305514421880632" />
      <inertia ixx="0.000692" ixy="0.0" ixz="0.000175" iyy="0.000805" iyz="-0.0" izz="0.000196" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.023226 0.006935 -0.127996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_2.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="silver" />
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.023226 0.006935 -0.127996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_2.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </collision>
  </link>

  <joint name="joint_2" type="revolute">
    <xacro:if value="${ns == 'left'}">
      <origin rpy="0 0 0" xyz="-0.02 0.012069 0.04316" />
    </xacro:if>
    <xacro:if value="${ns == 'right'}">
      <origin rpy="0 0 3.14159" xyz="0.02 0.012069 0.04316" />
    </xacro:if>
    <parent link="link_1" />
    <child link="link_2" />
    <axis xyz="1.0 -0.0 0.0" />
    <limit effort="100" lower="-1.658063" upper="1.658063" velocity="100" />
  </joint>

  <link name="link_3">
    <inertial>
      <origin rpy="0 0 0" xyz="0.01486624761711016 5.106898781712457e-05 0.01728082961823646" />
      <mass value="0.12173296266489733" />
      <inertia ixx="4.7e-05" ixy="-0.0" ixz="-6e-06" iyy="5e-05" iyz="0.0" izz="2.9e-05" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.013226 0.006935 -0.247996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_3.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="silver" />
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.013226 0.006935 -0.247996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_3.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </collision>
  </link>

  <joint name="joint_3" type="revolute">
    <origin rpy="0 0 0" xyz="0.01 0.0 0.12" />
    <parent link="link_2" />
    <child link="link_3" />
    <axis xyz="1.0 -0.0 0.0" />
    <limit effort="100" lower="-2.268928" upper="2.268928" velocity="100" />
  </joint>

  <link name="link_4">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.011101446329824061 3.2242915119862735e-05 0.04184113083783203" />
      <mass value="0.1955624220705575" />
      <inertia ixx="0.00022" ixy="-0.0" ixz="3.5e-05" iyy="0.000227" iyz="0.0" izz="5.5e-05" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="-0.006774 0.006935 -0.287996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_4.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="silver" />
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.006774 0.006935 -0.287996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_4.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </collision>
  </link>

  <joint name="joint_4" type="revolute">
    <origin rpy="0 0 0" xyz="0.02 0.0 0.04" />
    <parent link="link_3" />
    <child link="link_4" />
    <axis xyz="0.0 7e-06 1.0" />
    <limit effort="100" lower="-3.1415" upper="3.1415" velocity="100"/>
  </joint>

  
  <link name="link_5">
    <inertial>
      <origin rpy="0 0 0" xyz="0.02043962300807103 -1.5866844334157942e-07 0.010610125536728798" />
      <mass value="0.06937470143360883" />
      <inertia ixx="2.7e-05" ixy="-0.0" ixz="-2e-06" iyy="3.1e-05" iyz="-0.0" izz="1.7e-05" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.003226 0.006934 -0.377996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_5.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="silver" />
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.003226 0.006934 -0.377996" />
      <geometry>
        <mesh filename="package://arm2d2_description/meshes/link_5.stl" scale="0.001 0.001 0.001" />
      </geometry>
    </collision>
  </link>

  <joint name="joint_5" type="revolute">
    <origin rpy="0 0 0" xyz="-0.01 1e-06 0.09" />
    <parent link="link_4" />
    <child link="link_5" />
    <axis xyz="1.0 -0.0 0.0" />
    <limit effort="100" lower="-2.268928" upper="2.268928" velocity="100" />
  </joint>
  
  <transmission name="transmission_1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="transmission_2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="transmission_3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_3">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_3">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="transmission_4">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_4">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_4">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="transmission_5">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_5">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor_5">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <!--<robotNamespace>/</robotNamespace>-->
    </plugin>
  </gazebo>

  <gazebo reference="base_link">
    <material>Gazebo/Silver</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <selfCollide>true</selfCollide>
    <gravity>true</gravity>
  </gazebo>

  <gazebo reference="link_1">
    <material>Gazebo/Silver</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <selfCollide>true</selfCollide>
  </gazebo>

  <gazebo reference="link_2">
    <material>Gazebo/Silver</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <selfCollide>true</selfCollide>
  </gazebo>

  <gazebo reference="link_3">
    <material>Gazebo/Silver</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <selfCollide>true</selfCollide>
  </gazebo>

  <gazebo reference="link_4">
    <material>Gazebo/Silver</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <selfCollide>true</selfCollide>
  </gazebo>

  <gazebo reference="link_5">
    <material>Gazebo/Silver</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <selfCollide>true</selfCollide>
  </gazebo>


</robot>
